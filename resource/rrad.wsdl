<?xml version='1.0' encoding='UTF-8' ?>

<definitions name='RRAD'
  targetNamespace='urn:RRAD'
  xmlns:typens='urn:RRAD'
  xmlns:soap='http://schemas.xmlsoap.org/wsdl/soap/'
  xmlns:xsd='http://www.w3.org/2001/XMLSchema'
  xmlns:soapenc='http://schemas.xmlsoap.org/soap/encoding/'
  xmlns:wsdl='http://schemas.xmlsoap.org/wsdl/'
  xmlns='http://schemas.xmlsoap.org/wsdl/' >

	<!-- TYPES BEGIN -->

	<types>
	<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:RRAD">
		<xsd:complexType name="PrimaryInfo">
		<xsd:all>
			<xsd:element name="username" type="xsd:string"/>
			<xsd:element name="password" type="xsd:string"/>
			<xsd:element name="clientid" type="xsd:string" minOccurs="0"/>
			<xsd:element name="salesrep" type="xsd:string" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="ReturnCode">
		<xsd:all>
			<xsd:element name="success" type="xsd:boolean"/>
			<xsd:element name="message" type="xsd:string"/>
			<xsd:element name="number" type="xsd:int" minOccurs="0"/>
			<xsd:element name="data" type="xsd:string" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
<!-- add 20090406 for multiAccount-->
	<xsd:complexType name="AccountSetting">
		<xsd:all>
			<xsd:element name="sendinvoice" type="xsd:string" minOccurs="0"/>
			<xsd:element name="sendstatement" type="xsd:string" minOccurs="0"/>
			<xsd:element name="sendnews" type="xsd:string" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="AccountInfo">
		<xsd:all>
			<xsd:element name="accountid" type="xsd:string" minOccurs="0"/>
			<xsd:element name="username" type="xsd:string" minOccurs="0"/>
			<xsd:element name="email" type="xsd:string" minOccurs="0"/>
			<xsd:element name="domain" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ftpuser" type="xsd:string" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
<!-- add 20090406 for Mailinglist-->
<!-- add 20091112 for moderators flag-->
	<xsd:complexType name="MailinglistInfo">
		<xsd:all>
			<xsd:element name="account" type="xsd:string" />
			<xsd:element name="maintain" type="xsd:string" minOccurs="0"/>
			<xsd:element name="password" type="xsd:string" minOccurs="0"/>
			<xsd:element name="moderators" type="xsd:boolean" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
		<xsd:complexType name="DomainInfo">
		<xsd:all>
			<xsd:element name="password" type="xsd:string" minOccurs="0"/>
			<xsd:element name="package" type="xsd:string"/>
			<xsd:element name="contact" type="xsd:string" minOccurs="0"/>
			<xsd:element name="linkto" type="xsd:string" minOccurs="0"/>
<!-- add 20091127 for vms export to vms flag-->
			<xsd:element name="flag" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="accountInfo" type="typens:AccountInfo" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
	<xsd:complexType name="NewServiceData">
		<xsd:all>
		<xsd:element name="username" type="xsd:string" minOccurs="0"/>
		<xsd:element name="password" type="xsd:string" minOccurs="0"/>
		<xsd:element name="number" type="xsd:string" minOccurs="0"/>
		<xsd:element name="id" type="xsd:string" minOccurs="0"/>
		<xsd:element name="language" type="xsd:string" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
		<xsd:complexType name="NewService">
		<xsd:all>
			<xsd:element name="service" type="xsd:string" minOccurs="0"/>
			<xsd:element name="username" type="xsd:string" minOccurs="0"/>
			<xsd:element name="password" type="xsd:string" minOccurs="0"/>
			<xsd:element name="quantity" type="xsd:string" minOccurs="0"/>
			<xsd:element name="discount" type="xsd:string" minOccurs="0"/>
			<xsd:element name="comment" type="xsd:string" minOccurs="0"/>
			<xsd:element name="oldsid" type="xsd:string" minOccurs="0"/>
			<xsd:element name="data" type="typens:NewServiceData" minOccurs="0">
			</xsd:element>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="NewForward">
		<xsd:all>
			<xsd:element name="url" type="xsd:string"/>
			<xsd:element name="inframeset" type="xsd:string" minOccurs="0"/>
			<xsd:element name="catchall" type="xsd:string" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="DomainLimits">
		<xsd:all>
			<xsd:element name="bandwidth" type="xsd:int" minOccurs="0"/>
			<xsd:element name="quota" type="xsd:int" minOccurs="0"/>
			<xsd:element name="emails" type="xsd:int" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="CustomerInfo">
		<xsd:all>
			<xsd:element name="FirstName" type="xsd:string"/>
			<xsd:element name="LastName" type="xsd:string"/>
			<xsd:element name="Company" type="xsd:string"/>
			<xsd:element name="Address1" type="xsd:string"/>
			<xsd:element name="Address2" type="xsd:string"/>
			<xsd:element name="City" type="xsd:string"/>
			<xsd:element name="Province" type="xsd:string"/>
			<xsd:element name="PostalCode" type="xsd:string"/>
			<xsd:element name="Country" type="xsd:string"/>
			<xsd:element name="Email" type="xsd:string"/>
			<xsd:element name="Phone" type="xsd:string"/>
<!-- add 20090406 -->
			<xsd:element name="AltPhone" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Fax" type="xsd:string"/>
			<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="MailInfo">
		<xsd:all>
			<xsd:element name="account" type="xsd:string"/>
			<xsd:element name="password" type="xsd:string" minOccurs="0"/>
			<xsd:element name="alias" type="xsd:string" minOccurs="0"/>
			<xsd:element name="flag" type="xsd:boolean" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
<!-- add 20090210 for mail rotation -->
		<xsd:complexType name="RotationInfo">
		<xsd:all>
			<xsd:element name="account" type="xsd:string"/>
			<xsd:element name="days" type="xsd:int"/>
			<xsd:element name="folder" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="FTPInfo">
		<xsd:all>
			<xsd:element name="username" type="xsd:string"/>
			<xsd:element name="password" type="xsd:string" minOccurs="0"/>
			<xsd:element name="subdirectory" type="xsd:string" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="IntRange">
		<xsd:all>
			<xsd:element name="Start" type="xsd:int"/>
			<xsd:element name="End" type="xsd:int" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="AdminInfo">
		<xsd:all>
			<xsd:element name="SalesRep" type="xsd:string"/>
			<xsd:element name="ExternalID" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="ServiceInfo">
		<xsd:all>
			<xsd:element name="ServiceName" type="xsd:string"/>
			<xsd:element name="Status" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ServiceID" type="xsd:string" minOccurs="0"/>
<!-- add 20090716 -->
			<xsd:element name="Type" type="xsd:string" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="UsageInfo">
		<xsd:all>
			<xsd:element name="Month" type="xsd:string"/>
			<xsd:element name="Bandwidth" type="xsd:string"/>
			<xsd:element name="Diskusage" type="xsd:string"/>
			<xsd:element name="MailboxesUsed" type="xsd:string"/>
			<xsd:element name="Mailboxes" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="DNSAInfo">
		<xsd:all>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="Address" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="DNSCInfo">
		<xsd:all>
			<xsd:element name="Alias" type="xsd:string"/>
			<xsd:element name="Name" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="DNSMInfo">
		<xsd:all>
			<xsd:element name="Priority" type="xsd:string"/>
			<xsd:element name="Address" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>
<!-- add 20090224 for SPF-->
		<xsd:complexType name="DNSTInfo">
		<xsd:all>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="Value" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="Responder">
		<xsd:all>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="State" type="xsd:string"/>
			<xsd:element name="Condition" type="xsd:string"/>
			<xsd:element name="ConditionText" type="xsd:string"/>
			<xsd:element name="Content" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="CCInfo">
		<xsd:all>
			<xsd:element name="CardHolder" type="xsd:string"/>
			<xsd:element name="CardNumber" type="xsd:string"/>
			<xsd:element name="CardExpiry" type="xsd:string"/>
<!-- add 20090706-->
			<xsd:element name="AVSAddress" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AVSPostalCode" type="xsd:string" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="Filter">
		<xsd:all>
			<xsd:element name="Rule" type="xsd:string"/>
			<xsd:element name="OrderID" type="xsd:int" minOccurs="0"/>
			<xsd:element name="Status" type="xsd:string" minOccurs="0"/>
			<xsd:element name="DateFrom" type="xsd:string" minOccurs="0"/>
			<xsd:element name="DateTo" type="xsd:string" minOccurs="0"/>
			<xsd:element name="RuleName" type="xsd:string" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="MySQLInfo">
			<xsd:all>
				<xsd:element name="version" type="xsd:int"/>
				<xsd:element name="password" type="xsd:string" minOccurs="0"/>
				<xsd:element name="dbname" type="xsd:string" minOccurs="0"/>
			</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="MySQLUserInfo">
		<xsd:all>
			<xsd:element name="username" type="xsd:string"/>
		</xsd:all>
		</xsd:complexType>

		<!-- Arrays -->
		<xsd:complexType name="ArrayOfCCNumbers">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="xsd:string[]"/>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
		<xsd:complexType name="ArrayOfServiceInfo">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:ServiceInfo[]"/>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
		<xsd:complexType name="ArrayOfUsageInfo">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:UsageInfo[]"/>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
		<xsd:complexType name="ArrayOfDNSAInfo">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:DNSAInfo[]"/>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
		<xsd:complexType name="ArrayOfDNSCInfo">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:DNSCInfo[]"/>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
		<xsd:complexType name="ArrayOfDNSMInfo">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:DNSMInfo[]"/>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
<!-- add 20090224 for SPF-->
		<xsd:complexType name="ArrayOfDNSTInfo">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:DNSTInfo[]"/>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
		<xsd:complexType name="ArrayOfResponders">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:Responder[]"/>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
		<xsd:complexType name="ArrayOfAllow">
				<xsd:complexContent>
					<xsd:restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType"
							wsdl:arrayType="xsd:string[]"/>
					</xsd:restriction>
				</xsd:complexContent>
			</xsd:complexType>
		<xsd:complexType name="ArrayOfBlock">
				<xsd:complexContent>
					<xsd:restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType"
							wsdl:arrayType="xsd:string[]"/>
					</xsd:restriction>
				</xsd:complexContent>
			</xsd:complexType>
		<xsd:complexType name="ArrayOfLanguages">
				<xsd:complexContent>
					<xsd:restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType"
							wsdl:arrayType="xsd:string[]"/>
					</xsd:restriction>
				</xsd:complexContent>
			</xsd:complexType>
		<xsd:complexType name="ArrayOfStrings">
				<xsd:complexContent>
					<xsd:restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType"
							wsdl:arrayType="xsd:string[]"/>
					</xsd:restriction>
				</xsd:complexContent>
			</xsd:complexType>
		<xsd:complexType name="ArrayOfFilters">
				<xsd:complexContent>
					<xsd:restriction base="soapenc:Array">
						<attribute ref="soapenc:arrayType"
							wsdl:arrayType="typens:Filter[]"/>
					</xsd:restriction>
				</xsd:complexContent>
			</xsd:complexType>
		<!-- Spam/AutoRes -->
		<xsd:complexType name='AutoResponders'>
		<xsd:all>
			<xsd:element name='active' type='typens:ArrayOfResponders'/>
			<xsd:element name='inactive' type='typens:ArrayOfResponders'/>
		</xsd:all>
		</xsd:complexType>
		<xsd:complexType name="SpamFilter">
		<xsd:all>
			<xsd:element name="State" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SensitivityLevel" type="xsd:string" minOccurs="0"/>
			<xsd:element name="DeliveryMode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AllowList" type="typens:ArrayOfAllow" minOccurs="0"/>
			<xsd:element name="BlockList" type="typens:ArrayOfBlock" minOccurs="0"/>
			<xsd:element name="BlockedLanguages" type="typens:ArrayOfLanguages" minOccurs="0"/>
		</xsd:all>
		</xsd:complexType>

<!-- add 20081202 for f2e -->
	<xsd:complexType name="FaxInfo">
		<xsd:all>
			<xsd:element name="Domain" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Email" type="xsd:string" minOccurs="0"/>
			<xsd:element name="FaxNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ServiceID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ServiceCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="StartDate" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Status" type="xsd:string" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>

	<xsd:complexType name="ArrayOfFaxInfo">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:FaxInfo[]"/>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="FaxUsage">
		<xsd:all>
			<xsd:element name="month" type="xsd:string"/>		
			<xsd:element name="year" type="xsd:string"/>		
			<xsd:element name="PageLimit" type="xsd:int"/>
			<xsd:element name="PagesSent" type="xsd:int"/>
			<xsd:element name="PagesReceived" type="xsd:int"/>
		</xsd:all>
	</xsd:complexType>
	
	<xsd:complexType name="ArrayOfFaxUsage">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:FaxUsage[]"/>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="NewFaxReservation">
		<xsd:all>
			<xsd:element name="Email" type="xsd:string"/>
		</xsd:all>
	</xsd:complexType>

	<xsd:complexType name="FaxReservationInfo">
		<xsd:all>
			<xsd:element name="ReservationId" type="xsd:string"/>
			<xsd:element name="FaxNumbers" type="typens:ArrayOfStrings"/>
		</xsd:all>
	</xsd:complexType>

<!-- add 20090209 for f2e SubAccount-->
	<xsd:complexType name="FaxSubAccount">
		<xsd:all>
			<xsd:element name="ServiceID" type="xsd:string"/>
			<xsd:element name="Email" type="xsd:string"/>
			<xsd:element name="Password" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ReceiveFaxes" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="SendFaxes" type="xsd:boolean" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="ArrayOfFaxSubAccount">
		<xsd:complexContent>
			<xsd:restriction base="soapenc:Array">
			<attribute ref="soapenc:arrayType"
				wsdl:arrayType="typens:FaxSubAccount[]"/>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>

	</xsd:schema>
	</types>

	<!-- TYPES END -->

	<message name="helloRequest">
	<part name="name" type="xsd:string"/>
	</message>
	<message name="helloResponse">
	<part name="return" type="xsd:string"/>
	</message>


	<!-- MESSAGES BEGIN -->


	<!-- Web Commands -->
	<message name="newDomainRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="domaininfo" type="typens:DomainInfo"/>
	</message>
	<message name="newDomainResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="delDomainRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	</message>
	<message name="delDomainResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="setPasswordRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="password" type="xsd:string"/>
	</message>
	<message name="setPasswordResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>


	<!-- Admin Commands -->
	<message name="setPackageRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="domaininfo" type="typens:DomainInfo"/>
	</message>
	<message name="setPackageResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="setForwardRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="forward" type="typens:NewForward"/>
	</message>
	<message name="setForwardResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="setEmailsRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="emails" type="xsd:int"/>
	</message>
	<message name="setEmailsResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="setStorageRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="quota" type="xsd:int"/>
	</message>
	<message name="setStorageResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="setDomainLimitsRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="limits" type="typens:DomainLimits"/>
	</message>
	<message name="setDomainLimitsResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="setBandwidthRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="bandwidth" type="xsd:int"/>
	</message>
	<message name="setBandwidthResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="suspendWebRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="singular" type="xsd:boolean"/>
	</message>
	<message name="suspendWebResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="unsuspendWebRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="singular" type="xsd:boolean"/>
	</message>
	<message name="unsuspendWebResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="disableWebRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="singular" type="xsd:boolean"/>
	</message>
	<message name="disableWebResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="enableWebRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="singular" type="xsd:boolean"/>
	</message>
	<message name="enableWebResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="seterrorWebRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="error" type="xsd:int"/>
	</message>
	<message name="seterrorWebResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="restoreWebRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="error" type="xsd:int"/>
	</message>
	<message name="restoreWebResponse">
	<part name="return" type="typens:ReturnCode"/>
	</message>

	<message name="setVCPPRequest">
	<part name="primary" type="typens:PrimaryInfo"/>
	<part name="domain" type="xsd:string"/>
	<part name="custom" type="xsd:boolean"/>
	</message>
	<message name="setVCPPResponse">
